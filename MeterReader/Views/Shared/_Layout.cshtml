@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MeterReader</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/angular-material/angular-material.css"/>
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/angular-material/1.1.4/angular-material.min.css"
              asp-fallback-href="~/lib/angular-material/angular-material.min.css"
              asp-fallback-test-class="inset" asp-fallback-test-property="padding" asp-fallback-test-value="10px" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
    @RenderBody()
    @*<nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">MeterReader</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                </ul>
                @await Html.PartialAsync("_LoginPartial")
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - MeterReader</p>
        </footer>
    </div>*@

    <environment names="Development">

        <script src="~/lib/angular/angular.js"></script>
        <script src="~/lib/angular-animate/angular-animate.js"></script>
        <script src="~/lib/angular-aria/angular-aria.js"></script>
        <script src="~/lib/angular-messages/angular-messages.js"></script>
        <script src="~/lib/angular-ui-router/release/angular-ui-router.js"></script>
        <script src="~/lib/angular-material/angular-material.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>

        @*<!--Scripts that will be merged into site.min.js-->*@
        <script src="~/js/left/leftController.js"></script>
        <script src="~/js/main/landingController.js"></script>

    </environment>
    <environment names="Staging,Production">
        @*<!--angular-->*@
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"
                asp-fallback-src="~/lib/angular/angular.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>

        @*<!--angular-animate-->*@
        <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.4/angular-animate.min.js"
                asp-fallback-src="~/lib/angular-animate/angular-animate.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>

        @*<!--angular-aria-->*@
        <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.4/angular-aria.min.js"
                asp-fallback-src="~/lib/angular-aria/angular-aria.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>

        @*<!--angular-messages-->*@
        <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.4/angular-messages.min.js"
                asp-fallback-src="~/lib/angular-messages/angular-messages.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>

        @*<!--angular-ui-router-->*@
        <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/1.0.3/angular-ui-router.min.js"
                asp-fallback-src="~/lib/angular-ui-router/release/angular-ui-router.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>


        @*<!--angular-material-->*@
        <script src="https://ajax.googleapis.com/ajax/libs/angular_material/1.1.4/angular-material.min.js"
                asp-fallback-src="~/lib/angular-material/angular-material.min.js"
                asp-fallback-test="window.angular"
                crossorigin="anonymous"
                integrity="">
        </script>
        @*<!--application-->*@
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
